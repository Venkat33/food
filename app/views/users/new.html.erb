<h1>users</h1>
<% if @user.errors.any? %>
<ul>
<% @user.errors.full_messages.each do |msg| %>
<li><%= msg %> </li>
<% end %>
</ul>
<% end %>

<%= form_for @user do |f| %>
<p>
  <%= f.label :username%><br/>
  <%= f.text_field :username %>
</p>
<p>
<%= f.label :email %><br/>
<%= f.text_field :email %>
</p>
<p>

    <%= f.label :password_digest %>
    <%= f.password_field :password_digest%></br>
</p>

<%= f.submit "submit" %>
</p>
<%end %>
